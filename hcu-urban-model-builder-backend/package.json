{
  "name": "hcu-urban-model-builder-backend",
  "description": "",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {},
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 20.13.1"
  },
  "type": "module",
  "feathers": {
    "language": "ts",
    "packager": "npm",
    "database": "postgresql",
    "framework": "koa",
    "transports": [
      "rest",
      "websockets"
    ],
    "schema": "typebox"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "lib/client.js",
    "lib/**/*.d.ts",
    "lib/**/*.shared.js"
  ],
  "main": "lib/client",
  "scripts": {
    "dev": "nodemon --watch \"src/**\" --ext \"ts\"  -x node --import ./ts-node.register.mjs src/index.ts",
    "debug": "nodemon --watch \"src/**\" --ext \"ts\"  -x node --inspect --import ./ts-node.register.mjs src/index.ts",
    "compile": "shx rm -rf lib/ && tsc",
    "start": "node lib/",
    "prettier": "npx prettier \"**/*.ts\" --write",
    "mocha": "cross-env NODE_ENV=test mocha test/ --require ts-node/register --recursive --extension .ts --exit",
    "test": "cross-env NODE_ENV=test npm run migrate && npm run mocha",
    "bundle:client": "npm run compile && npm pack --pack-destination ./public",
    "migrate": "npm run knex -- migrate:latest",
    "migrate:make": "npm run knex -- migrate:make",
    "knex": "node --import ./ts-node.register.mjs ./node_modules/.bin/knex"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^5.0.29",
    "@feathersjs/authentication": "^5.0.29",
    "@feathersjs/authentication-client": "^5.0.29",
    "@feathersjs/authentication-local": "^5.0.30",
    "@feathersjs/authentication-oauth": "^5.0.30",
    "@feathersjs/configuration": "^5.0.29",
    "@feathersjs/errors": "^5.0.29",
    "@feathersjs/feathers": "^5.0.29",
    "@feathersjs/knex": "^5.0.29",
    "@feathersjs/koa": "^5.0.29",
    "@feathersjs/memory": "^5.0.29",
    "@feathersjs/schema": "^5.0.29",
    "@feathersjs/socketio": "^5.0.29",
    "@feathersjs/transport-commons": "^5.0.29",
    "@feathersjs/typebox": "^5.0.29",
    "feathers-authentication-oidc": "^0.4.4",
    "feathers-keycloak-connect": "^1.3.8",
    "knex": "^3.1.0",
    "pg": "^8.12.0",
    "simulation": "^5.0.0",
    "simulation-viz-console": "^1.0.5",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@feathersjs/cli": "^5.0.29",
    "@feathersjs/rest-client": "^5.0.29",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.16.1",
    "axios": "^1.7.4",
    "cross-env": "^7.0.3",
    "mocha": "^10.7.3",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
