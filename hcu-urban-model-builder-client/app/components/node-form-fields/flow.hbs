<@form.element
  @controlType="textarea"
  @label="Rate"
  @property="data.rate" as |el|
>
  <NodeFormFields::Formula
    @form={{@form}}
    @node={{@node}}
    @el={{el}}
    @disabled={{@disabled}}
  >
    <el.control class="code-textarea" />
  </NodeFormFields::Formula>
</@form.element>

<NodeFormFields::ParameterSettings
  @form={{@form}}
  @changeset={{@changeset}}
  @el={{@el}}
  @errors={{@errors}}
  @type="numeric"
  @outputVisible={{true}}
  @disabled={{@disabled}}
/>

<NodeFormFields::Validation
  @modelsVersion={{@modelsVersion}}
  @form={{@form}}
  @changeset={{@changeset}}
  @el={{@el}}
/>